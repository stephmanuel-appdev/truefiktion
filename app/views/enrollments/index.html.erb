<div class="page-header">
  <h1>
    Enrollments
    <a href="/enrollments/new" class="btn btn-lg btn-success">New Enrollment</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Issue</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @enrollments.each do |enrollment| %>
      <tr>
        <td><%= enrollment.issue.user_id %></td>
        <td><%= enrollment.user_id %></td>
        <td>
          <a href="/enrollments/<%= enrollment.id %>" class="btn btn-primary">Show</a>
          <a href="/enrollments/<%= enrollment.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_enrollment/<%= enrollment.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
